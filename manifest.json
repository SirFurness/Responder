    "web_accessible_resources": ["*.jpg","*.JPG", "*.png"],
    "content_scripts": [{
        "matches": ["*://hangouts.google.com/*"],
        "js": ["listenForChatWindow.js", "insertButton.js", "removeButton.js", "insertMessage.js"]
    }]
}
